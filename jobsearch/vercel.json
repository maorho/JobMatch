{
  "functions": {
    "api/external-jobs/cleanup-now/route.ts": {
      "runtime": "nodejs20.x"
    },
    "api/external-jobs/sync-if-stale/route.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "crons": [
    {
      "path": "/api/external-jobs/cleanup-now",
      "schedule": "0 */48 * * *"
    },
    {
      "path": "/api/external-jobs/sync-if-stale",
      "schedule": "0 */6 * * *"
    }
  ]
}
